{
  "name": "@mlaursen/eslint-config",
  "version": "12.0.4",
  "description": "An eslint config used by mlaursen for most projects.",
  "type": "module",
  "exports": {
    ".": "./dist/index.js",
    "./package.json": "./package.json"
  },
  "scripts": {
    "prepare": "test -f dist/index.js || pnpm build",
    "clean-dist": "rm -rf dist",
    "clean-cache": "rm -rf .turbo  node_modules",
    "clean": "concurrently 'pnpm clean-dist' 'pnpm clean-cache'",
    "lint": "eslint .",
    "typecheck": "tsc --noEmit",
    "typecheck-watch": "pnpm typecheck --watch",
    "check-format": "prettier --check .",
    "format": "prettier --write .",
    "build": "tsc -p tsconfig.build.json"
  },
  "author": "Mikkel Laursen <mlaursen03@gmail.com>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mlaursen/npm-packages.git",
    "directory": "packages/eslint-config"
  },
  "bugs": {
    "url": "https://github.com/mlaursen/npm-packages/issues"
  },
  "keywords": [
    "eslint",
    "typescript"
  ],
  "license": "MIT",
  "dependencies": {
    "@eslint/compat": "^2.0.2",
    "@eslint/core": "^1.1.0",
    "@eslint/js": "^9.39.3",
    "@types/eslint-plugin-jsx-a11y": "^6.10.1",
    "@typescript-eslint/utils": "^8.56.1",
    "@vitest/eslint-plugin": "^1.6.9",
    "eslint": "^9.39.3",
    "eslint-plugin-jest": "^29.15.0",
    "eslint-plugin-jest-dom": "^5.5.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.5.2",
    "eslint-plugin-testing-library": "^7.16.0",
    "eslint-plugin-unicorn": "^63.0.0",
    "typescript-eslint": "^8.56.1"
  },
  "devDependencies": {
    "@types/node": "^25.3.2",
    "concurrently": "^9.2.1",
    "prettier": "^3.8.1",
    "typescript": "^5.9.3"
  },
  "peerDependencies": {
    "eslint": ">= 9.0.0",
    "typescript": ">= 5"
  },
  "peerDependenciesMeta": {
    "eslint": {
      "optional": false
    },
    "typescript": {
      "optional": true
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "volta": {
    "node": "24.13.1",
    "pnpm": "10.29.3"
  }
}
