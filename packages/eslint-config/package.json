{
  "name": "@mlaursen/eslint-config",
  "version": "10.0.2",
  "description": "An eslint config used by mlaursen for most projects.",
  "type": "module",
  "exports": {
    ".": "./dist/index.js",
    "./package.json": "./package.json"
  },
  "scripts": {
    "prepare": "test -f dist/index.js || pnpm build",
    "clean-dist": "rm -rf dist",
    "clean-cache": "rm -rf .turbo  node_modules",
    "clean": "concurrently 'pnpm clean-dist' 'pnpm clean-cache'",
    "lint": "eslint .",
    "typecheck": "tsc --noEmit",
    "typecheck-watch": "pnpm typecheck --watch",
    "check-format": "prettier --check .",
    "format": "prettier --write .",
    "build": "tsc -p tsconfig.build.json"
  },
  "author": "Mikkel Laursen <mlaursen03@gmail.com>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mlaursen/npm-packages.git",
    "directory": "packages/eslint-config"
  },
  "bugs": {
    "url": "https://github.com/mlaursen/npm-packages/issues"
  },
  "keywords": [
    "eslint",
    "typescript"
  ],
  "license": "MIT",
  "dependencies": {
    "@eslint/compat": "^1.4.1",
    "@eslint/core": "^0.17.0",
    "@eslint/js": "^9.39.0",
    "@types/eslint-plugin-jsx-a11y": "^6.10.1",
    "@typescript-eslint/utils": "^8.46.2",
    "@vitest/eslint-plugin": "^1.4.0",
    "eslint": "^9.39.0",
    "eslint-plugin-jest": "^29.0.1",
    "eslint-plugin-jest-dom": "^5.5.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-testing-library": "^7.13.3",
    "typescript-eslint": "^8.46.2"
  },
  "devDependencies": {
    "@mlaursen/release-script": "workspace:*",
    "@types/node": "^24.9.2",
    "concurrently": "^9.2.1",
    "prettier": "^3.6.2",
    "typescript": "^5.9.3"
  },
  "peerDependencies": {
    "eslint": ">= 9.0.0",
    "typescript": ">= 5"
  },
  "peerDependenciesMeta": {
    "eslint": {
      "optional": false
    },
    "typescript": {
      "optional": true
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "volta": {
    "node": "24.11.0",
    "pnpm": "10.20.0"
  }
}
