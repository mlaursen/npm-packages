{
  "name": "vite",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "clean-dist": "rm -rf dist",
    "clean-cache": "rm -rf .turbo node_modules",
    "clean": "concurrently 'pnpm clean-dist' 'pnpm clean-cache'",
    "check-format": "prettier --check .",
    "format": "prettier --write .",
    "lint": "eslint .",
    "lint-fix": "pnpm lint --fix",
    "test": "vitest",
    "test-compiler": "REACT_COMPILER=true vitest",
    "test-coverage": "vitest run --coverage",
    "test-coverage-compiler": "pnpm test-compiler run --coverage",
    "dev": "vite",
    "dev-compiler": "REACT_COMPILER=true vite",
    "build": "tsc -b && vite build",
    "build-compiler": "tsc -b && REACT_COMPILER=true vite build",
    "preview": "vite preview"
  },
  "dependencies": {
    "@mlaursen/code": "workspace:*",
    "@react-md/core": "^6.3.4",
    "cnbuilder": "^3.1.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1"
  },
  "devDependencies": {
    "@mlaursen/eslint-config": "workspace:*",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@trivago/prettier-plugin-sort-imports": "^6.0.0",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "@vitejs/plugin-react": "^5.1.1",
    "@vitejs/plugin-react-swc": "^4.2.2",
    "@vitest/coverage-v8": "^4.0.15",
    "babel-plugin-react-compiler": "^1.0.0",
    "concurrently": "^9.2.1",
    "eslint": "^9.39.1",
    "eslint-plugin-react-refresh": "^0.4.22",
    "jsdom": "^27.2.0",
    "prettier": "^3.7.4",
    "sass-embedded": "^1.93.3",
    "typescript": "~5.9.3",
    "vite": "^7.2.6",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^4.0.15"
  },
  "volta": {
    "node": "24.11.0",
    "pnpm": "10.20.0"
  }
}
