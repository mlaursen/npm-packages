{
  "name": "vite-app",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "clean-dist": "rm -rf dist",
    "clean-cache": "rm -rf .turbo node_modules",
    "clean": "concurrently 'pnpm clean-dist' 'pnpm clean-cache'",
    "check-format": "prettier --check .",
    "format": "prettier --write .",
    "lint": "eslint .",
    "lint-fix": "pnpm lint --fix",
    "test": "vitest",
    "test-coverage": "vitest --coverage",
    "test-run": "vitest run",
    "test-run-snapshot": "vitest run -u",
    "test-run-coverage": "vitest run --coverage",
    "test-compiler": "REACT_COMPILER=true vitest",
    "test-coverage-compiler": "pnpm test-compiler run --coverage",
    "dev": "vite",
    "dev-compiler": "REACT_COMPILER=true vite",
    "build": "tsc -b && vite build",
    "build-compiler": "tsc -b && REACT_COMPILER=true vite build",
    "preview": "vite preview"
  },
  "dependencies": {
    "@mlaursen/code": "workspace:*",
    "@react-md/core": "^7.0.1",
    "cnbuilder": "^3.1.0",
    "react": "^19.2.3",
    "react-dom": "^19.2.3"
  },
  "devDependencies": {
    "@mlaursen/eslint-config": "workspace:*",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.1",
    "@testing-library/user-event": "^14.6.1",
    "@trivago/prettier-plugin-sort-imports": "^6.0.2",
    "@types/node": "^25.0.9",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "@vitejs/plugin-react": "^5.1.2",
    "@vitejs/plugin-react-swc": "^4.2.2",
    "@vitest/coverage-v8": "^4.0.17",
    "babel-plugin-react-compiler": "^1.0.0",
    "concurrently": "^9.2.1",
    "eslint": "^9.39.2",
    "eslint-plugin-react-refresh": "^0.4.26",
    "jsdom": "^27.4.0",
    "prettier": "^3.8.0",
    "sass-embedded": "^1.97.2",
    "typescript": "~5.9.3",
    "vite": "^7.3.1",
    "vite-tsconfig-paths": "^6.0.4",
    "vitest": "^4.0.17"
  },
  "volta": {
    "node": "24.12.0",
    "pnpm": "10.26.1"
  }
}
