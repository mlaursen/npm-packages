{
  "name": "next-app",
  "type": "module",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "clean-dist": "rm -rf .next",
    "clean-cache": "rm -rf .turbo node_modules",
    "clean": "concurrently 'pnpm clean-dist' 'pnpm clean-cache'",
    "check-format": "prettier --check .",
    "format": "prettier --write .",
    "lint": "eslint .",
    "lint-fix": "pnpm lint --fix",
    "test": "vitest",
    "test-coverage": "vitest --coverage",
    "test-run": "vitest run",
    "test-run-snapshot": "vitest run -u",
    "test-run-coverage": "vitest run --coverage",
    "dev": "next dev",
    "build": "next build",
    "start": "next start"
  },
  "dependencies": {
    "@mlaursen/code": "workspace:*",
    "@react-md/core": "^6.5.0",
    "cnbuilder": "^3.1.0",
    "next": "^16.1.0",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "shiki": "^3.20.0"
  },
  "devDependencies": {
    "@mlaursen/eslint-config": "workspace:*",
    "@next/eslint-plugin-next": "^16.1.0",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.1",
    "@testing-library/user-event": "^14.6.1",
    "@trivago/prettier-plugin-sort-imports": "^6.0.0",
    "@types/node": "^25.0.3",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "@vitejs/plugin-react": "^5.1.2",
    "@vitest/coverage-v8": "^4.0.16",
    "babel-plugin-react-compiler": "^1.0.0",
    "concurrently": "^9.2.1",
    "eslint": "^9.39.2",
    "jsdom": "^27.3.0",
    "prettier": "^3.7.4",
    "sass-embedded": "^1.97.0",
    "typescript": "^5.9.3",
    "vite-tsconfig-paths": "^6.0.3",
    "vitest": "^4.0.16"
  },
  "volta": {
    "node": "24.12.0",
    "pnpm": "10.26.1"
  }
}
